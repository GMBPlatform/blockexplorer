      <form id="search-form__header" method="GET" onsubmit="return searchHeaderOnSubmit()">
        <div class="navbar-search navbar__menu-search">
          <div class="input-group input-group-icon iconfont icon-right">
            <div class="input-group-prepend">
              <button
                id="searchHeaderDropDown"
                class="btn btn-outline-secondary dropdown-toggle"
                type="button"
                data-toggle="dropdown"
                aria-haspopup="true"
                aria-expanded="false"
              >
                Filter
              </button>
              <div
                class="dropdown-menu"
                x-placement="bottom-start"
                style="position: absolute; transform: translate3d(0px, 36px, 0px); top: 0px; left: 0px; will-change: transform;"
              >
                <a class="dropdown-item" onclick="searchHeaderDropDownItemOnClick('block');">Block</a>
                <a class="dropdown-item" onclick="searchHeaderDropDownItemOnClick('transaction');">Tx</a>
                <a class="dropdown-item" onclick="searchHeaderDropDownItemOnClick('wallet');">Wallet</a>
              </div>
            </div>
            <input
              id="search-input__header"
              class="form-control navbar-search__input navbar__menu-search-input"
              name="key"
              type="text"
              placeholder="Search"
            /><button
              class="input-icon ua-icon-search header-search__form"
              onmouseover="$(this).css('cursor', 'pointer');"
            ></button>
          </div>
        </div>
      </form>
    </div>
  </div>
